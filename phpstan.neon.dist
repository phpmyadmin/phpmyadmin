includes:
    - phpstan-baseline.neon
parameters:
    level: max
    paths:
        - .
    scanFiles:
        - libraries/vendor_config.php
    bootstrapFiles:
        - test/phpstan-constants.php
    excludePaths:
        - examples/openid.php
        - node_modules/*
        - libraries/cache/*
        - test/doctum-config.php
        - tmp/*
        - twig-templates/*
        - vendor/*
    dynamicConstantNames:
        - ROOT_PATH
        - VERSION_SUFFIX
