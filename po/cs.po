msgid "Bookmark this SQL query"
msgstr "Přidat tento SQL dotaz do oblíbených"

#: libraries/sql.lib.php:765
msgid "Label:"
msgstr "Název:"

#: libraries/sql.lib.php:773 libraries/sql_query_form.lib.php:327
msgid "Let every user access this bookmark"
msgstr "Umožnit všem uživatelům používat tuto oblíbenou položku"

#: libraries/sql.lib.php:1025
msgid "Bookmark not created"
msgstr "Nebyl vytvořen oblíbený dotaz"

#: libraries/sql.lib.php:1183
#, php-format
msgid "Using bookmark \"%s\" as default browse query."
msgstr "Pro procházení databáze je použit oblíbený dotaz „%s\"."

#: libraries/sql.lib.php:1628
msgid "Showing as PHP code"
msgstr "Zobrazuji jako PHP kód"

#: libraries/sql.lib.php:1633
msgid "Validated SQL"
msgstr "Proběhlo zkontrolování SQL"

#: libraries/sql.lib.php:1911
#, php-format
msgid ""
"Table %s does not contain a unique column. Grid edit, checkbox, Edit, Copy "
"and Delete features are not available."
msgstr ""
