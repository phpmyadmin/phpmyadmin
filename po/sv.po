msgid "Space usage"
msgstr "Utrymmesanvändning"

#: libraries/messages.inc.php:1398
msgid "Spanish"
msgstr "Spanska"

#: libraries/messages.inc.php:1399
msgid "Words are separated by a space character (\" \")."
msgstr "Ord separeras med mellanslag (\" \")."

